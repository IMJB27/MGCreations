
@{
    ViewBag.Title = "User_Manage_Order";
}

<h2>User_Manage_Order</h2>

<div class="container-fluid py-4">
    <h4 class="text-center text-uppercase">Order List</h4>
    <table class="table table-striped table-bordered">
        <thead class="bg-dark text-white">
            <tr>
                <th>Order Reference No.</th>
                <th>Order Date</th>
                <th>User Name</th>
                <th>Product Name</th>
                <th>Product Quantity</th>
                <th>Product Price</th>
                <th>Order Total</th>
                <th>Order Status</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Order.Order_Reference_No</td>
                    <td>@item.Order.Order_Date</td>
                    <td>@item.User.User_Username</td>
                    <td>@item.Product.Product_Name</td>
                    <td>@item.Cart.Product_Quantity</td>
                    <td>@item.Product.Product_Price</td>
                    <td>@item.Order.Order_TotalAmount</td>
                    <td>@item.Order.Order_Status</td>
                       
                </tr>

            }
        </tbody>
    </table>
</div>